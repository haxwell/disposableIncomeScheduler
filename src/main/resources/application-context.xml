<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="MenuItemHandlers" class="com.haxwell.disposableIncomeScheduler.beans.MenuItemHandlerBean">
		<property name="children">
			<list>
				<ref bean="AddAGoalMenuItemHandler"/>
				<ref bean="RemoveAGoalMenuItemHandler"/>
				<ref bean="EditAGoalMenuItemHandler"/>
				<ref bean="EditCalculationVariablesMenuItemHandler"/>
				<ref bean="AddThisPeriodsSavedAmountToEachEntryMenuItemHandler"/>
				<ref bean="ListTheScheduleMenuItemHandler"/>
			</list>
		</property>
	</bean>

	<bean id="AddAGoalMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.AddAGoalMenuItemHandler">
	</bean>

	<bean id="RemoveAGoalMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.RemoveAGoalMenuItemHandler">
	</bean>

	<bean id="EditAGoalMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.EditAGoalMenuItemHandler">
	</bean>

	<bean id="EditCalculationVariablesMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.EditCalculationVariablesMenuItemHandler">
	</bean>

	<bean id="AddThisPeriodsSavedAmountToEachEntryMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.AddThisPeriodsSavedAmountToEachEntryMenuItemHandler">
	</bean>

	<bean id="ListTheScheduleMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.ListTheScheduleMenuItemHandler">
	</bean>
	
	<bean id="MenuItemHandlerProviderCollection" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.MenuItemHandlerProviderCollection">
		<property name="list">
			<list>
				<ref bean="SelectAGroupMenuItemHandlerProvider"/>
				<ref bean="MoveToParentGroupMenuItemHandlerProvider"/>
				<ref bean="AddAGroupMenuItemHandlerProvider"/>
				<ref bean="EditCurrentGroupMenuItemHandlerProvider"/>
				<ref bean="RemoveSelectedGroupMenuItemHandlerProvider"/>
				<ref bean="ListGoalsMenuItemHandlerProvider"/>
				<ref bean="AddAGoalMenuItemHandlerProvider"/>
				<ref bean="RemoveAGoalMenuItemHandlerProvider"/>
				<ref bean="EditAGoalMenuItemHandlerProvider"/>
				<ref bean="AddAnExpenseMenuItemHandlerProvider"/>
				<ref bean="EditAnExpenseMenuItemHandlerProvider"/>
				<ref bean="RemoveAnExpenseMenuItemHandlerProvider"/>
				<ref bean="ListExpensesMenuItemHandlerProvider"/>
				<ref bean="EditCalculationVariablesMenuItemHandlerProvider"/>
				<ref bean="ApplyThisPeriodsSavedAmountToGoalsMenuItemHandlerProvider"/>
				<ref bean="ListTheScheduleMenuItemHandlerProvider"/>
			</list>
		</property>
	</bean>

	<bean id="SelectAGroupMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.SelectAGroupMenuItemHandlerProvider">
	</bean>

	<bean id="MoveToParentGroupMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.MoveToParentGroupMenuItemHandlerProvider">
	</bean>

	<bean id="AddAGroupMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.AddAGroupMenuItemHandlerProvider">
	</bean>

	<bean id="EditCurrentGroupMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.EditCurrentGroupMenuItemHandlerProvider">
	</bean>

	<bean id="RemoveSelectedGroupMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.RemoveSelectedGroupMenuItemHandlerProvider">
	</bean>
	
	<bean id="RemoveAGoalMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.RemoveAGoalMenuItemHandlerProvider">
	</bean>
	
	<bean id="AddAGoalMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.AddAGoalMenuItemHandlerProvider">
	</bean>
	
	<bean id="EditAGoalMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.EditAGoalMenuItemHandlerProvider">
	</bean>

	<bean id="ListGoalsMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.ListGoalsMenuItemHandlerProvider">
	</bean>

	<bean id="ApplyThisPeriodsSavedAmountToGoalsMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.ApplyThisPeriodsSavedAmountToGoalsMenuItemHandlerProvider">
	</bean>

	<bean id="EditCalculationVariablesMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.EditCalculationVariablesMenuItemHandlerProvider">
	</bean>

	<bean id="ListTheScheduleMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.ListTheScheduleMenuItemHandlerProvider">
	</bean>
	
	<bean id="AddAnExpenseMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.AddAnExpenseMenuItemHandlerProvider">
	</bean>
	
	<bean id="EditAnExpenseMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.EditAnExpenseMenuItemHandlerProvider">
	</bean>
	
	<bean id="RemoveAnExpenseMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.RemoveAnExpenseMenuItemHandlerProvider">
	</bean>

	<bean id="ListExpensesMenuItemHandlerProvider" class="com.haxwell.disposableIncomeScheduler.beans.menuItemHandlerProviders.ListExpensesMenuItemHandlerProvider">
	</bean>

</beans>