<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="MenuItemHandlers" class="com.haxwell.disposableIncomeScheduler.beans.MenuItemHandlerBean">
		<property name="children">
			<list>
				<ref bean="AddAnEntryMenuItemHandler"/>
				<ref bean="RemoveAnEntryMenuItemHandler"/>
				<ref bean="EditAnEntryMenuItemHandler"/>
				<ref bean="EditCalculationVariablesMenuItemHandler"/>
				<ref bean="AddThisMonthsSavedAmountToEachEntryMenuItemHandler"/>
				<ref bean="ListTheScheduleMenuItemHandler"/>
			</list>
		</property>
	</bean>

	<bean id="AddAnEntryMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.AddAnEntryMenuItemHandler">
	</bean>

	<bean id="RemoveAnEntryMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.RemoveAnEntryMenuItemHandler">
	</bean>

	<bean id="EditAnEntryMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.EditAnEntryMenuItemHandler">
	</bean>

	<bean id="EditCalculationVariablesMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.EditCalculationVariablesMenuItemHandler">
	</bean>

	<bean id="AddThisMonthsSavedAmountToEachEntryMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.AddThisMonthsSavedAmountToEachEntryMenuItemHandler">
	</bean>

	<bean id="ListTheScheduleMenuItemHandler" class="com.haxwell.disposableIncomeScheduler.beans.ListTheScheduleMenuItemHandler">
	</bean>

</beans>